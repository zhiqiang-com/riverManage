            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="时间：" />
        <TextView
            android:id="@+id/Tv_adss"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="描述：" />

        <TextView
            android:id="@+id/Tv_miaoshu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    </LinearLayout>


